! Xresources file

Xcursor.theme: DMZ (Black)
Xcursor.size: 24

URxvt.transparent: true
URxvt.shading: 25
URxvt.font: xft:Noto Sans Mono:pixelsize=14:antialias=true, xft:FontAwesome:pixelsize=14:antialias=true
URxvt*boldFont: xft:Noto Sans Mono:bold:pixelsize=14:antialias=true, xft:FontAwesome:pixelsize=14:antialias=true
URxvt.letterSpace: -1

URxvt.perl-ext-common: default,matcher,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5

! Background color
URxvt*background: rgb:22/22/22
URxvt*blurRadius: 5

! Font color
URxvt*foreground: rgb:ee/ee/ee

! Other colors
URxvt*color0: rgb:55/57/53
URxvt*color1: rgb:ef/29/29
URxvt*color2: rgb:8a/e2/34
URxvt*color3: rgb:fc/e9/4f
URxvt*color4: rgb:72/9f/cf
URxvt*color5: rgb:ad/7f/a8
URxvt*color6: rgb:34/e2/e2
URxvt*color7: rgb:ee/ee/ee
URxvt*color8: rgb:55/57/53
URxvt*color9: rgb:ef/29/29
URxvt*color10: rgb:8a/e2/34
URxvt*color11: rgb:fc/e9/4f
URxvt*color12: rgb:72/9f/cf
URxvt*color13: rgb:ad/7f/a8
URxvt*color14: rgb:34/e2/e2
URxvt*color15: rgb:ee/ee/ee

URxvt*.scrollBar: false

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up

URxvt.iso14755: false
URxvt.iso14755_52: false
!URxvt.borderLess: true

URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

URxvt.keysym.C-minus:		resize-font:smaller
URxvt.keysym.C-plus:		resize-font:bigger
URxvt.keysym.C-equal:		resize-font:reset
URxvt.keysym.C-question:	resize-font:show
